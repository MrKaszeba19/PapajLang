@use(Math)
@use(Array)

"Enter coord of the bridge start: " print
scan toNumber trunc -> a
"Enter coord of the bridge end:   " print
scan toNumber trunc -> b

a b - abs -> len
len 6 / -> span

"Bridge length:  " print 
$len println
"Segment length: " print
$span println

"Segments:       " print
[ 1 $span $len ++ seq ] fun{ $b + -- } map println

