"TEST LOOPS" println
"TEST 1" println
1 -> i
while ( i 5 <= ) {
	i println
	i ++ -> i
}

"TEST 2" println
0 -> i
do {
	i ++ -> i
	i println
} while ( i 5 < )

"TEST 3" println
0 -> i
do {
	i ++ -> i
	i println
} until ( i 5 >= )

"TEST 4" println
for ( 1 -> $i ; $i 5 <= ; $i ++ -> $i ) {
	$i println
}

"TEST 5" println
for ( $i : [ 1 2 3 4 5 ] ) {
	$i println
}

"TEST 6" println
for ( const $i : [ 1 2 3 4 5 ] ) {
	$i println
}
